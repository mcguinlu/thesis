@article{rea2005,
  title = {Statin {{Use}} and the {{Risk}} of {{Incident Dementia}}: The {{Cardiovascular Health Study}}},
  shorttitle = {Statin {{Use}} and the {{Risk}} of {{Incident Dementia}}},
  author = {Rea, Thomas D. and Breitner, John C. and Psaty, Bruce M. and Fitzpatrick, Annette L. and Lopez, Oscar L. and Newman, Anne B. and Hazzard, William R. and Zandi, Peter P. and Burke, Gregory L. and Lyketsos, Constantine G. and Bernick, Charles and Kuller, Lewis H.},
  year = {2005},
