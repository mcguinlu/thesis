  langid = {english},
  pmid = {34702754},
  file = {C\:\\Users\\lm16564\\Zotero\\storage\\DEUKGXH2\\Skrivankova et al. - 2021 - Strengthening the reporting of observational studi.pdf;C\:\\Users\\lm16564\\Zotero\\storage\\QTBZJLJT\\bmj.html}
