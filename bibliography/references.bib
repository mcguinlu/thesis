@article{kirkham2020,
  title = {Systematic Examination of Preprint Platforms for Use in the Medical and Biomedical Sciences Setting},
  author = {Kirkham, Jamie J. and Penfold, Naomi C. and Murphy, Fiona and Boutron, Isabelle and Ioannidis, John P. and Polka, Jessica and Moher, David},
