@article{mathur2014,
  title = {Completeness and Usability of Ethnicity Data in {{UK}}-Based Primary Care and Hospital Databases},
  author = {Mathur, Rohini and Bhaskaran, Krishnan and Chaturvedi, Nish and Leon, David A. and {vanStaa}, Tjeerd and Grundy, Emily and Smeeth, Liam},
