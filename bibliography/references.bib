@article{russ2014,
  title = {Height in Relation to Dementia Death: Individual Participant Meta-Analysis of 18 {{UK}} Prospective Cohort Studies},
  shorttitle = {Height in Relation to Dementia Death},
  author = {Russ, Tom C. and Kivim{\"a}ki, Mika and Starr, John M. and Stamatakis, Emmanuel and Batty, G. David},
