  author = {Letenneur, L. and Gilleron, V. and Commenges, D. and Helmer, C. and Orgogozo, J. M. and Dartigues, J. F.},
  year = {1999},
  month = feb,
  journal = {Journal of Neurology, Neurosurgery \& Psychiatry},
  volume = {66},
