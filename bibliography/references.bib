  file = {C\:\\Users\\lm16564\\Zotero\\storage\\H9NRLWMV\\Kirkham et al. - 2020 - Systematic examination of preprint platforms for u.pdf;C\:\\Users\\lm16564\\Zotero\\storage\\QN7E3JVR\\e041849.html},
  journal = {BMJ Open},
  keywords = {medical ethics,medical journalism,statistics \& research methods},
  language = {en},
  number = {12},
  pmid = {33376175}
}

