@article{farmer2018,
  title = {Promises and Pitfalls of Electronic Health Record Analysis},
  author = {Farmer, Ruth and Mathur, Rohini and Bhaskaran, Krishnan and Eastwood, Sophie V. and Chaturvedi, Nish and Smeeth, Liam},
