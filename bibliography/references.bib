  author = {Li, G. and Shofer, J. B. and Kukull, W. A. and Peskind, E. R. and Tsuang, D. W. and Breitner, J. C. S. and McCormick, W. and Bowen, J. D. and Teri, L. and Schellenberg, G. D. and Larson, E. B.},
  year = {2005},
