}

@article{stone2020,
  title = {Comparison of Bias Adjustment Methods in Meta-Analysis Suggests That Quality Effects Modeling May Have Less Limitations than Other Approaches},
  author = {Stone, Jennifer C. and Glass, Kathryn and Munn, Zachary and Tugwell, Peter and Doi, Suhail A. R.},
  year = {2020},
  month = jan,
  journal = {Journal of Clinical Epidemiology},
