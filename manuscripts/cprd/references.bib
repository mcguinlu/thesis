  file = {C\:\\Users\\lm16564\\Zotero\\storage\\PHGCD7YY\\Wright et al. - 2017 - Life Expectancy and Cause-Specific Mortality in Ty.pdf;C\:\\Users\\lm16564\\Zotero\\storage\\UWDCFXDP\\338.html},
