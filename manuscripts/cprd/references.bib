  shorttitle = {Common Medications and Survival in Women with Ovarian Cancer},
  author = {Majidi, Azam and Na, Renhua and {Dixon-Suen}, Suzanne and Jordan, Susan J. and Webb, Penelope M.},
  year = {2020},
